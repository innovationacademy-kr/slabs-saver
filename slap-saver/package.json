{
  "name": "slap-saver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pm2-dev ./bin/www",
    "node": "node_modules/.bin/nodemon ./bin/www",
    "db": "cd db && docker-compose up",
    "dbclear": "rm -rf db/srcs/data",
    "dbreset": "npm run clear && npm run db",
    "do": "node_modules/.bin/sequelize db:migrate && sequelize db:seed:all",
    "undo": "node_modules/.bin/sequelize db:seed:undo:all && sequelize db:migrate:undo:all",
    "test": "node_modules/.bin/jest"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.22.0",
    "alert": "^5.0.10",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "mariadb": "^2.5.3",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.5",
    "@editorjs/image": "^2.6.0",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/nested-list": "^1.0.1",
    "aws-sdk": "^2.930.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.7"
  }
}
