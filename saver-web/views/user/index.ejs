<main class="main">
  <div class="today-card ice-blue-two">
    <!--오늘의뉴스-->
    <a id="today_article_link" href="">
      <div class="today-card__title today_article">
        <i class="today-card__title__icon logo-type-small-blue"></i>
        <h3 class="today-card__title__text">오늘의 뉴스</h3>
      </div>
      <p id="today_article" class="today-card__text ft-main"></p>
    </a>
    <div class="divide-white"></div>

    <!--오늘의 한마디-->
    <div class="today-card__title today_word">
      <i class="today-card__title__icon logo-small-blue"></i>
      <h3 class="today-card__title__text">오늘의 한마디</h3>
    </div>
    <p id="today_word" class="today-card__text ft-main"></p>
  </div>

  <div class="article-choice">
    <button id= "article-all" class="button is-checked">All Section</button>
    <button id="article-category" class="button">My Section</button>
  </div>

  <div id="article-list" class="article-list"></div>
  <div id="article-category-list" class="article-list"></div>
</main>
